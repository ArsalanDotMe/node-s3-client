{
  "name": "modern-s3",
  "version": "0.1.2",
  "description": "Modern Amazon S3 client for high-level operations",
  "main": "lib/index",
  "types": "lib/index",
  "scripts": {
    "test": "jest --runInBand",
    "build": "tsc -p .",
    "build:live": "tsc -w",
    "security": "nsp check --summary",
