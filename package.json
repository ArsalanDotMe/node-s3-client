  "author": "Arsalan Ahmad",
  "license": "MIT",
  "engines": {
    "node": ">=8.0.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "@types/mkdirp": "^0.5.2",
    "@types/node": "^12.7.11",
    "@types/rimraf": "^2.0.2",
    "jest": "^24.9.0",
    "mocha": "^5.2.0",
    "ncp": "^2.0.0",
    "ts-jest": "^24.1.0",
    "tslint": "^5.20.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.6.3"
  },
  "dependencies": {
    "aws-sdk": "^2.346.0",
    "fd-slicer": "^1.1.0",
    "findit2": "~2.2.3",
